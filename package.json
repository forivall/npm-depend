{
  "name": "npm-depend",
  "version": "0.0.0",
  "description": "CLI tool to add dependencies to your package.json without installation",
  "license": "ISC",
  "author": "Emily Marigold Klassen <forivall@gmail.com>",
  "homepage": "https://github.com/forivall/npm-depend",
  "repository": {
    "type": "git",
    "url": "git@github.com:forivall/npm-depend.git"
  },
  "bugs": {
    "url": "https://github.com/forivall/npm-depend/issues"
  },
  "index": "index.js",
  "bin": {
    "npm-depend": "./npm-depend.js"
  },
  "scripts": {
    "lint": "xo",
    "lint-staged": "lint-staged",
    "inspect": "jsinspect",
    "test": "ava",
    "posttest": "npm run lint && npm run inspect",
    "test-cov": "nyc ava",
    "posttest-cov": "npm run posttest",
    "test-ci": "npm run test-cov",
    "posttest-ci": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [],
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "dependencies": {
    "read-pkg": "^2.0.0",
    "read-pkg-up": "^2.0.0",
    "resolve-pkg": "^1.0.0",
    "write-pkg": "^2.1.0",
    "yargs": "^7.0.2"
  },
  "bundledDependencies": [],
  "peerDependencies": {},
  "devDependencies": {
    "ava": "^0.18.2",
    "coveralls": "^2.11.14",
    "eslint-config-mxo": "^0.4.0",
    "jsinspect": "^0.10.1",
    "nyc": "^10.1.2",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": [
      "mxo"
    ],
    "overrides": [
      {
        "files": [
          "test/**",
          "test.js",
          "test-*.js",
          "**/__tests__/**",
          "**/*.test.js",
          "!**/fixtures/**",
          "!**/helpers/**"
        ],
        "extends": [
          "mxo/ava"
        ]
      }
    ]
  }
}
